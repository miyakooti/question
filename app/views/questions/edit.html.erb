<%= stylesheet_link_tag 'test' %>
<%= stylesheet_link_tag 'edit_new' %>
<div id="container">
<h1 class="main_title">編集</h1>
<%= form_with model:@question, local: true do |form|%>
<div class="question">
    <div class="quiz">
      <p><%= form.label :quiz, "問題文" %></p>
      <p><%= form.text_area :quiz %></p>
    </div>
    
    <div class="answer">
      <p><%= form.label :answer, "回答" %></p>
      <p><%= form.text_area :answer %></p>
    </div>
    
    <div class="submit">
    <%= form.submit "保存する" %>
    </div>
    
</div>
</div>
<% end %>