<h1>新規作成</h1>
<%= form_with model:@question, local: true do |form|%>
<div class="question">
    <div class="quiz">
      <p><%= form.label :quiz, "問題文" %></p>
      <p><%= form.text_field :quiz %></p>
    </div>
    
    <div class="answer">
      <p><%= form.label :answer, "回答" %></p>
      <p><%= form.text_field :answer %></p>
    </div>
    
    <div>
    <%= form.submit "新規作成" %>
    </div>
    
</div>
<% end %>